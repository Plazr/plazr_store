<div class="row">
  <div class="span12">
    <h1>PlazrStore</h1>
  </div>
</div>


<!-- Promotional Video -->
<div id="promotions">
  <video></video>
</div>

<div class="row">
  <!-- featured products -->
  <div id="featured" class="span9">
    <div class="row">
      <% 10.times do %>
        <div class="span3">
          <%= render 'product' %>
        </div>
      <% end %>
    </div>
  </div>

  <!-- top -->
  <div class="span3">
    <h4>Top</h4>
    <% 10.times do %>
      <%= render 'top' %>
    <% end %>
</div>

